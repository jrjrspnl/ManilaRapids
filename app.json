{
  "expo": {
    "name": "MANILA RAPIDS",
    "slug": "manilarapids",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mrapids-logo-1024x1024.png",
    "scheme": "manilarapids",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/mrapids-logo-1024x1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mrapids-logo-1024x1024.png",
          "imageWidth": 60,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
